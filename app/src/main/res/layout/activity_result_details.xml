            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text=""
            android:gravity="left"
            android:layout_weight="1"
            android:id="@+id/textViewClassName"
            />
